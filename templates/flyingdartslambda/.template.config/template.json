  "classifications": [
    "Flyingdarts",
    "TopLevelLambda"
  ],
  "identity": "Flying.Lambda.Template",
  "name": "Flyingdarts template: Lambda",
  "shortName": "flyinglambda",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "(Flying.Function.Name)",
